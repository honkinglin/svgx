{
  "name": "vite-plugin-svgtidy",
  "version": "0.1.0",
  "description": "Vite plugin for svgtidy",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --shims"
  },
  "keywords": [
    "vite",
    "svg",
    "optimizer",
    "svgtidy"
  ],
  "author": "Kyrie Lin",
  "license": "MIT",
  "dependencies": {
    "svgtidy": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": ">=3.0.0"
  }
}